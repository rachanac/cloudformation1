[www]

user = deploy
group = deploy

listen = /run/php/php8.0-fpm.sock

listen.owner = deploy
listen.group = deploy

pm = dynamic
pm.max_children = 250
pm.start_servers = 5
pm.min_spare_servers = 5
pm.max_spare_servers = 35
pm.max_requests = 900
pm.status_path = /status

request_terminate_timeout = 10000
